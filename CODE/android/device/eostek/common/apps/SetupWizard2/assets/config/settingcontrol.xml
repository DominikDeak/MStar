<scpd>
    <serviceStateTable>
        <stateVariable sendEvents="yes">
            <name>LastChange</name>
            <dataType>string</dataType>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>A_ARG_TYPE_Channel</name>
            <dataType>string</dataType>
            <allowedValueList>
                <allowedValue>Master</allowedValue>
            </allowedValueList>
            <defaultValue>Master</defaultValue>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>PresetNameList</name>
            <dataType>string</dataType>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>A_ARG_TYPE_InstanceID</name>
            <dataType>ui4</dataType>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>A_ARG_TYPE_PresetName</name>
            <dataType>string</dataType>
            <allowedValueList>
                <allowedValue>FactoryDefaults</allowedValue>
                <allowedValue>InstallationDefaults</allowedValue>
            </allowedValueList>
            <defaultValue>FactoryDefaults</defaultValue>
        </stateVariable>
    </serviceStateTable>
    <actionList>
        <action>
            <name>RequestPairingCode</name>
            <argumentList>
                <argument>
                  <name>InstanceID</name>
                  <direction>in</direction>
                  <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
				<argument>
                    <name>PairingCode</name>
                    <direction>out</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>ConfirmPairingCode</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
				<argument>
                    <name>PairingCode</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetWifi</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
                <argument>
                    <name>Ssid</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
				<argument>
                    <name>SecurityMode</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
				<argument>
                    <name>Pw</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>IsAdvanced</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>AssignmentType</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>Ip</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>PrefixLen</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>GateWay</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetTimeZone</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
                <argument>
                    <name>TimeZone </name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>RestoreFactory</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
                <argument>
                    <name>Factory </name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetAdNet</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
                <argument>
                    <name>Proxy</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
				<argument>
                    <name>IpType</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
				<argument>
                    <name>Ip</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
                <argument>
                    <name>Gateway</name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
		<action>
            <name>SetNickName</name>
            <argumentList>
                <argument>
                    <name>InstanceID</name>
                    <direction>in</direction>
                    <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
                </argument>
                <argument>
                    <name>NickName </name>
                    <direction>in</direction>
                    <relatedStateVariable>PresetNameList</relatedStateVariable>
                </argument>
            </argumentList>
        </action>      
    </actionList>
</scpd>
