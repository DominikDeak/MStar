<?xml version="1.0" encoding="gb2312"?>
<config device_name="BenQi500" device_branch="dlp">
	
	<!-- others -->
	<Group name="others" enable="true">
		<Item name="amta_hdcp_key" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)HDCP KEY</property>
		</Item>
		<Item name="white_balance" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)白平衡</property>
		</Item>
		<Item name="hdmi1" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)HDMI1</property>
			<property name="sub_group">video</property>
			<property name="source">hdmi3</property>
			<property name="operate_tip">将输入源接到HDMI1接口，观看输出窗口的画面显示是否正常</property>
		</Item>
		<Item name="adc_adjust" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)ADC Adjust</property>			
			<property name="source">vga</property>
			<property name="operate_tip">接入ADC自动校正信号，点击“调整”自动进行校正</property>
		</Item>		
		<Item name="vga" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)VGA</property>
			<property name="sub_group">video</property>
			<property name="source">vga</property>
			<property name="operate_tip">将输入源接到VGA接口，观看输出窗口的画面显示是否正常</property>
		</Item>						
		<Item name="line_in" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)line_in</property>
			<property name="sub_group">video</property>
			<property name="source">vga</property>
			<property name="operate_tip">接入VGA，并接入音频输入信号，听喇叭声音是否正常</property>
		</Item>	
		<Item name="microphone_in" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)麦克风</property>
			<property name="sub_group">video</property>
			<property name="source">ktv</property>
			<property name="operate_tip">接上麦克风，听喇叭是否能够听到麦克风的声音</property>
		</Item>
		<Item name="amta_usb1" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)USB1</property>
			<property name="sub_group">storage</property>
			<property name="device_id">4</property>
		</Item>
		<Item name="amta_usb2" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)USB2</property>
			<property name="sub_group">storage</property>
			<property name="device_id">1</property>
		</Item>	
		<Item name="3d" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)3D</property>
		</Item>		
		<Item name="line_out" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)LineOut</property>
			<property name="operate_tip">接上耳机，听耳机是否有声音输出</property>
		</Item>		
		<Item name="amta_ntc_sensor" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)温度传感器</property>			
		</Item>	
		<Item name="fan_speed" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)风扇转速</property>
		</Item>		
		<Item name="wifi" enable="true">
			<property name="tag">manual</property>
			<property name="display_name">(手动)WIFI</property>
			<property name="show_ap_list">true</property>
			<property name="connect_ap">true</property>
		</Item>		
		<Item name="gravity_sensor" enable="true">
			<property name="tag">auto</property>
			<property name="display_name">(自动)重力传感器</property>
		</Item>		
		<Item name="eth" enable="true">
			<property name="tag">auto</property>
			<property name="display_name">(自动)以太网</property>
		</Item>
		
		<Item name="reset" enable="true">
			<property name="tag">special</property>
			<property name="display_name">重置</property>
		</Item>
		
	</Group>	
	<Group name="finish" enable="true">
		<Item name="finish" enable="false"/>
	</Group>
</config>
